{
  "name": "rolis/archetype",
  "description": "A modern attribute-based framework for WordPress plugin development",
  "version": "1.2.0",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Vitalii Sili",
      "email": "vitaliisili@yahoo.com",
      "homepage": "https://vitaliisili.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "analog/analog": "^1.0",
    "illuminate/database": "^12.17.0",
    "illuminate/events": "^12.17.0",
    "illuminate/container": "^12.17.0",
    "illuminate/support": "^12.17.0",
    "ramsey/uuid": "^4.7.6",
    "doctrine/dbal": "^3.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Archetype\\": "src/",
      "Archetype\\Dependencies\\": "src/Dependencies/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Archetype\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "mozart": {
      "dep_namespace": "Archetype\\Dependencies\\",
      "dep_directory": "/src/Dependencies/",
      "classmap_directory": "/src/Dependencies/classes/",
      "classmap_prefix": "Archetype_Dep_",
      "packages": "auto-discover",
      "excluded_packages": [],
      "delete_vendor_directories": false
    },
    "wordpress": {
      "tested": "6.4",
      "requires": "5.0"
    }
  },
  "minimum-stability": "stable"
}