{
  "name": "rolis/archetype",
  "description": "A modern attribute-based framework for WordPress plugin development with bundled dependencies",
  "version": "1.0.14",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Vitalii Sili",
      "email": "vitaliisili@yahoo.com",
      "homepage": "https://vitaliisili.com"
    }
  ],
  "require": {
    "php": ">=8.2"
  },
  "autoload": {
    "psr-4": {
      "Archetype\\": "src/"
    },
    "files": [
      "bootstrap/autoload.php"
    ]
  },
  "extra": {
    "bundled-dependencies": [
      "illuminate/bus",
      "illuminate/collections",
      "illuminate/conditionable",
      "illuminate/container",
      "illuminate/contracts",
      "illuminate/database",
      "illuminate/events",
      "illuminate/macroable",
      "illuminate/pipeline",
      "illuminate/support",
      "laravel/serializable-closure",
      "doctrine/inflector",
      "doctrine/dbal",
      "symfony/clock",
      "symfony/polyfill-mbstring",
      "symfony/translation",
      "psr/container",
      "brick/math",
      "nesbot/carbon",
      "ramsey/uuid",
      "analog/analog"
    ],
    "archetype": {
      "version": "1.0.14",
      "build-date": "2025-06-01",
      "bundle-size": "~6.5MB",
      "conflict-free": true,
      "illuminate-version": "^12.0"
    }
  },
  "keywords": [
    "wordpress",
    "plugin",
    "framework",
    "bundled",
    "conflict-free",
    "illuminate",
    "orm",
    "rest-api",
    "attributes"
  ],
  "scripts": {
    "post-install-cmd": [
      "echo 'Archetype framework installed with bundled dependencies - no conflicts!'"
    ]
  }
}